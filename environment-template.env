# Cursor AI Configuration Template
# Copy this file to .env and fill in your actual values

# MCP Server Configuration
JIRA_INSTANCE_URL=https://your-company.atlassian.net
JIRA_USER_EMAIL=your.email@company.com
JIRA_API_KEY=your_generated_api_token

POSTGRES_CONNECTION_STRING=postgresql://username:password@localhost:5432/database

BRAVE_API_KEY=your_brave_api_key

# Slack Integration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# GitHub Integration
GITHUB_PERSONAL_ACCESS_TOKEN=your-github-token

# Admin API
CURSOR_ADMIN_API_KEY=your_cursor_admin_api_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Security & Authentication
SAML_IDP_URL=https://your-sso-provider.com/saml
SAML_ENTITY_ID=cursor-ai-app
SAML_CERTIFICATE=your-saml-certificate

SCIM_ENDPOINT=https://your-scim-endpoint.com
SCIM_TOKEN=your-scim-token

# Monitoring & Logging
SIEM_ENDPOINT=https://your-siem-endpoint.com
LOG_LEVEL=info

# Application Settings
NODE_ENV=production
PORT=3000
DEBUG=false

# Database
DATABASE_URL=your-database-url
REDIS_URL=your-redis-url

# Email Notifications
SMTP_HOST=your-smtp-host
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASS=your-smtp-password

# Additional API Keys
STRIPE_API_KEY=your-stripe-api-key
SHOPIFY_API_KEY=your-shopify-api-key 